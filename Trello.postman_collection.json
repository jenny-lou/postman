		{
			"name": "Create a List",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let idList = pm.response.json().id\r",
							"let idListName = pm.response.json().name\r",
							"if(idListName == \"DONE\"){\r",
							"    pm.collectionVariables.set(\"idListDone\", idList);\r",
							"} else {\r",
							"    pm.collectionVariables.set(\"idListTodo\", idList);\r",
							"}\r",
							"\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"isClosed\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.closed).to.eql(false);\r",
							"});\r",
							"\r",
							"pm.test(\"check list Nameis TODO\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.name).to.eql(\"TODO\");\r",
							"});\r",
							"\r",
							"pm.test(\"check list Name is DONE\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.name).to.eql(\"DONE\");\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
