		{
			"name": "Create a Board",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let idBoard = pm.response.json().id\r",
							"pm.collectionVariables.set(\"idBoard\", idBoard);\r",
							"let boardName = pm.environment.get(\"boardName\");\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"pm.test(\"Board should be created\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.name).to.eql(`${boardName}`);\r",
							"});\r",
							"\r",
							"pm.test(\"Permission level is private\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.prefs.permissionLevel).to.eql(\"private\");\r",
							"});"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"// function range(start, end) {\r",
							"//   return Array(end - start + 1).fill().map((_, idx) => start + idx)\r",
							"// }\r",
							"\r",
							"let currNum = pm.environment.get(\"boardNum\");\r",
							"pm.environment.unset(\"boardName\");\r",
							"\r",
							"let newNum = currNum + 1\r",
							"pm.environment.set(\"boardNum\", newNum);\r",
							"pm.environment.set(\"boardName\", `My Board ${newNum}`);\r",
							"\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"url": {
					"raw": "{{baseUrl}}/:version/boards/?name={{boardName}}&key={{apiKey}}&token={{token}}&defaultLists=false",
					"host": [
						"{{baseUrl}}"
					],
					"path": [
						":version",
						"boards",
						""
					],
					"query": [
						{
							"key": "name",
							"value": "{{boardName}}"
						},
