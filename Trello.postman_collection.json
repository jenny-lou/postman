					"script": {
						"exec": [
							"let id_card = pm.response.id;\r",
							"let id_board = pm.collectionVariables.get(\"idBoard\");\r",
							"let id_list_todo = pm.collectionVariables.get(\"idListToDo\");\r",
							"let id_list_done = pm.collectionVariables.get(\"idListDone\")\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"isClosed\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.closed).to.eql(false);\r",
							"});\r",
							"\r",
							"pm.test(\"check card Name\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.name).to.eql(\"Learn Postman\");\r",
							"});\r",
							"\r",
							"pm.test(\"check id Board\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.idBoard).to.eql(id_board);\r",
							"});\r",
							"\r",
							"pm.test(\"check list id is TODO\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.idList).to.eql(id_list_todo);\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"check list id is DONE\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.idList).to.eql(id_list_done);\r",
							"});"
